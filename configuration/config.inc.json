{
  "domains": [
    "digitalcheckup.org",
    "digitalcheckup.local",
    "housing.digitalcheckup.org",
    "digitalcheckup-housing.local"
  ],
  "routes": [
    "{:./routes/*.json}"
  ],
  "metaData": {
    "assetsVersion": "{:../package.json@version}",
    "assetsBase": "https://storage.googleapis.com/scvo-assets/test/digitalcheckup/",
    "title": "Digital Checkup",
    "domain-map": "{:./data/domain-map.json}",
    "site-configurations": "{:./data/site-configurations.json}",
    "menus": "{:./data/menus.json}",
    "checkup": "{:./data/checkup.json}",
    "areas": "{:./data/areas.json}",
    "elasticsearchUrl": "{$./data/elasticsearch-url.txt}",
    "handlebars": {
      "partials": {
        "{$./partials/**/*.*}": "includeRelativePath=false&regexExpression=(-)&regexOptions=gi&regexReplace=_"
      },
      "templates": {
        "{$./templates/**/*.*}": ""
      },
      "layouts": {
        "{$./layouts/**/*.*}": ""
      },
      "queries": {
        "{$./queries/**/*.*}": ""
      }
    },
    "commonTasks": "{:./data/common-tasks.json}"
  }
}