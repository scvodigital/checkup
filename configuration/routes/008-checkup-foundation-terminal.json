{
  "name": "checkup_foundation-terminal",
  "pattern": {
    "checkup_foundation_terminal": "/checkup/foundation-terminal",
    "life_foundation_terminal": "/life/foundation-terminal",
    "work_foundation_terminal": "/work/foundation-terminal"
  },
  "tasks": [
    "context.metaData.commonTasks.currentSite",
    "context.metaData.commonTasks.currentCheckup",
    {
      "name": "destination",
      "taskModule": "renderLayout",
      "renderer": "handlebars",
      "config": {
        "logic": {
          "if": [
            "default"
          ]
        },
        "output": "body",
        "layouts": {
          "default": {
            "layout": ">context.metaData.handlebars.layouts.default",
            "parts": {
              "head": ">context.metaData.handlebars.templates.head-tags",
              "body": ">context.metaData.handlebars.templates.checkup_foundation-terminal"
            },
            "contentType": "text/html"
          }
        }
      }
    }
  ]
}